<div class="card">
    <img class="card-img-top" src="<%= clip.image %>" style = "width: 100%;height: 200px" alt="Card image cap">
      <div class="card-body">
       <h5 class="card-title"><%= clip.title %></h5>
      <p class="card-text"><%= clip.description %></p>
      <%= link_to 'Show', clip %>
      <%= link_to 'Edit', edit_clip_path(clip) %>
      <%= link_to 'Destroy', clip, method: :delete, data: { confirm: 'Are you sure?' } %>
    </div>
</div>

